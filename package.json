{
  "name": "sprintboard",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "license": "AGPLv2",
  "scripts": {
    "server:dev": "tsx src/server/index.ts",
    "server:start": "tsx src/server/index.ts",
    "ui:dev": "vite",
    "codegen:sql": "pnpm run pgtyped",
    "codegen:graphql:server": "grats --tsconfig tsconfig.node.json",
    "codegen:graphql:client": "genql --schema schema.graphql --output src/ui/api-client/",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "dbmate": "dbmate",
    "pgtyped": "dotenv -- pgtyped -c pgtyped.config.json"
  },
  "dependencies": {
    "@pgtyped/runtime": "^2.4.2",
    "@signaldb/core": "^1.5.2",
    "@signaldb/sync": "^1.2.2",
    "@signaldb/vue": "^1.0.0",
    "@vueuse/core": "^13.1.0",
    "camelcase-keys": "^9.1.3",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "fluent-piper": "^3.1.0",
    "graphql": "^16.10.0",
    "graphql-yoga": "^5.13.3",
    "naive-ui": "^2.41.0",
    "pg": "^8.14.1",
    "pug": "^3.0.3",
    "remeda": "^2.21.2",
    "sass": "^1.86.3",
    "string-ts": "^2.2.1",
    "vfonts": "^0.0.3",
    "villus": "^3.5.2",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@genql/cli": "^6.3.3",
    "@pgtyped/cli": "^2.4.3",
    "@types/express": "^5.0.1",
    "@types/node": "^22.14.1",
    "@types/pg": "^8.11.12",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/language-plugin-pug": "^2.2.8",
    "@vue/tsconfig": "^0.7.0",
    "dbmate": "^2.26.0",
    "dotenv-cli": "^8.0.0",
    "grats": "^0.0.32",
    "tsx": "^4.19.3",
    "typescript": "~5.8.3",
    "vite": "^6.2.6",
    "vue-tsc": "^2.2.8"
  }
}
